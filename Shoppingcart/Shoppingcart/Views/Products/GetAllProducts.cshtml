@model IList<Product>

 <h1>Hi, these are my Products</h1>

<table>
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Product Description</th>
            <th>Product Id</th>
            <th>Product Image</th>
            <th>Product Price</th>
        </tr>
    </thead>
        
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td><span>@item.ProductName</span></td>
            <td><span>@item.ProductDescription</span></td>
            <td><span>@item.ProductId</span></td>
            <td><span>@item.ProductImage</span></td>
            <td><span>@item.ProductPrice</span></td>
        </tr>
        }
    </tbody>
</table>
 
      
 
